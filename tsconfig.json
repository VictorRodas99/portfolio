{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@icons/*": [
        "src/components/icons/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@client-scripts/*": [
        "src/scripts/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@css/*": [
        "src/css/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}